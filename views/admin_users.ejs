<!DOCTYPE html>
<html lang="ja">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/top.css'/>
</head>
<body>

<% include _header %>

<div class="main">
    <div class="container">
        <div class="row">
            <h1><%= displayTitle %></h1>

            <h2>Users</h2>
            <ul>
                <% users.forEach(function (user) { %>
                <li><%= user.username %></li>
                <% }); %>
            </ul>

            <h2>Valid Usernames</h2>
            <ul>
                <% validUsernames.forEach(function (username) { %>
                <li><%= username %></li>
                <% }); %>
            </ul>

            <h2>Admin Usernames</h2>
            <ul>
                <% adminUsernames.forEach(function (username) { %>
                <li><%= username %></li>
                <% }); %>
            </ul>
        </div>
    </div>
</div>

<!-- include javascripts -->
<script src="/vendor/jquery/dist/jquery.min.js"></script>
<script src="/vendor/bootstrap/dist/js/bootstrap.min.js"></script>

</body>
</html>
